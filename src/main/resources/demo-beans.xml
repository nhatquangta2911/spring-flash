<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

        <bean id="student" class="ryan.Student">
            <constructor-arg value="Hazel"/>
            <constructor-arg value="9.4"/>
            <constructor-arg ref="class"/>
        </bean>
        
        <bean id="class" class="ryan.Class">
            <property name="className" value="15T1"/>
        </bean>

        <bean id="singleton" class="ryan.BeanScopeSingleton">
        </bean>

        <bean id="prototype" class="ryan.BeanScopePrototype" scope="prototype">
        </bean>


</beans>